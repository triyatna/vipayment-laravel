{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "triyatna/vipayment-laravel",
    "description": "Library Laravel untuk integrasi dengan VIP Reseller API.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "vipayment",
        "laravel",
        "php",
        "vip reseller",
        "api vip reseller",
        "h2h vip reseller"
    ],
    "homepage": "https://github.com/triyatna/vipayment-laravel",
    "authors": [
        {
            "name": "Triyatna",
            "email": "triyatna.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Triyatna\\Vipayment\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Triyatna\\Vipayment\\VipaymentServiceProvider"
            ],
            "aliases": {
                "Vipayment": "Triyatna\\Vipayment\\Facades\\Vipayment"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
